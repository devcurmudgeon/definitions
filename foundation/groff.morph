name: foundation/groff.morph
kind: chunk
repo: upstream:groff
ref: c7017a099f954bb6de60e79c876935b1bf438b9a
max-jobs: 1
configure-commands:
- PAGE=A4 ./configure --prefix="$PREFIX"
build-commands:
- make
install-commands:
- make DESTDIR="$DESTDIR" install
- ln -s eqn "$DESTDIR$PREFIX/bin/geqn"
- ln -s tbl "$DESTDIR$PREFIX/bin/gtbl"
