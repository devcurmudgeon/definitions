name: ragel
kind: chunk
repo: upstream:ragel
ref: 5d4102ffe43a15608e0e864fc7e7f5c6c3b49aac
unpetrify-ref: baserock/morph
max-jobs: 1
build-system: autotools
configure-commands:
- ./autogen.sh --disable-manual --prefix="$PREFIX"
