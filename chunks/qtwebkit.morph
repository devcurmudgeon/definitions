repo: upstream:qt5/qtwebkit
ref: 15a558c03ea7a35f97dfc0b16024d026d20920be
unpetrify-ref: baserock/morph
max-jobs: '1'
configure-commands:
- QMAKEPATH=/qtwebkit.build/Tools/qmake/mkspecs qmake WebKit.pro
